<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="146.0" prefWidth="204.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ahhasc.View.Component.TechnicianCard">
   <children>
      <ImageView fitHeight="44.0" fitWidth="44.0" pickOnBounds="true" preserveRatio="true">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <image>
            <Image url="@../../material/credentialcardicon.png" />
         </image>
      </ImageView>
      <Label fx:id="technicianCountText" alignment="CENTER" prefHeight="20.0" prefWidth="146.0" text="Technician 1" textFill="#7c7c7c">
         <font>
            <Font name="Nunito Bold" size="13.0" />
         </font>
         <VBox.margin>
            <Insets top="17.0" />
         </VBox.margin>
      </Label>
      <Label fx:id="technicianNameText" alignment="CENTER" prefHeight="20.0" prefWidth="136.0" text="Ben Lim" textFill="#8d8d8d">
         <font>
            <Font name="Nunito Regular" size="13.0" />
         </font>
      </Label>
      <Label fx:id="technicianContactText" alignment="CENTER" prefHeight="17.0" prefWidth="196.0" text="0123456789" textFill="#8d8d8d">
         <font>
            <Font name="Nunito Regular" size="12.0" />
         </font>
      </Label>
      <Label fx:id="technicianSpecializationText" alignment="CENTER" prefHeight="17.0" prefWidth="196.0" text="Electronic" textFill="#8d8d8d">
         <font>
            <Font name="Nunito Regular" size="12.0" />
         </font>
      </Label>
   </children>
</VBox>
